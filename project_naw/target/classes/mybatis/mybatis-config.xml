<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- java beans 별명 짓기 -->
	<typeAliases>
		<typeAlias type="cart.bean.CartDTO" alias="cartDTO"/>
		<typeAlias type="review.bean.ReviewDTO" alias="reviewDTO"/>
		<typeAlias type="inquriy.bean.InquiryDTO" alias="inquiryDTO"/>
	</typeAliases>
	
	<!-- mapper 설정 -->
	<mappers>
		<mapper resource="mybatis/cart-mapping.xml"/>
		<mapper resource="mybatis/review-mapping.xml"/>
		<mapper resource="mybatis/inquiry-mapping.xml"/>
	</mappers>
	
</configuration>